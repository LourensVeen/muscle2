PROJECT_NAME           = "Muscle C/C++ API"
PROJECT_NUMBER         = "Version 2.0"
OUTPUT_DIRECTORY       = @MUSCLE_CPP_API@
WARNINGS               = NO
OPTIMIZE_OUTPUT_FOR_C  = YES
INPUT 		             = @PROJECT_SOURCE_DIR@/../src/cpp/muscle2
FILE_PATTERNS          = *.c *.h *.cpp *.hpp
EXCLUDE                = communicator.hpp xdr_communicator.hpp communicator.cpp xdr_communicator.cpp

RECURSIVE              = YES
SEARCHENGINE           = YES
GENERATE_LATEX         = NO
SORT_MEMBER_DOCS       = NO
EXTRACT_ALL            = YES
EXTRACT_STATIC         = YES
HIDE_UNDOC_MEMBERS     = NO
HIDE_UNDOC_CLASSES     = NO
HIDE_FRIEND_COMPOUNDS  = NO
HIDE_IN_BODY_DOCS      = NO
INTERNAL_DOCS          = NO
CASE_SENSE_NAMES       = NO
HIDE_SCOPE_NAMES       = NO
SHOW_INCLUDE_FILES     = YES
INLINE_INFO            = YES
SORT_MEMBER_DOCS       = YES
SORT_BRIEF_DOCS        = NO
SORT_BY_SCOPE_NAME     = NO
MAX_INITIALIZER_LINES  = 30
SHOW_DIRECTORIES       = YES
FULL_PATH_NAMES        = NO
SHOW_USED_FILES        = NO
