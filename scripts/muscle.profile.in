#!/bin/sh


if [[ -z $MUSCLE_HOME ]] ; then
	MUSCLE_HOME=_PREFIX_
fi

export MUSCLE_HOME
export PATH=$MUSCLE_HOME/bin:$PATH

if [ `uname` == "Darwin" ]
then
	export DYLD_LIBRARY_PATH=$MUSCLE_HOME/lib:$DYLD_LIBRARY_PATH
else 
	export LD_LIBRARY_PATH=$MUSCLE_HOME/lib:$LD_LIBRARY_PATH
fi

if [ -f $MUSCLE_HOME/etc/muscle.defaults ]
then
	. $MUSCLE_HOME/etc/muscle.defaults
fi
